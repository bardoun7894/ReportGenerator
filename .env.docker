# Docker Environment Variables Template
# Copy this to .env.local and fill in your values before running docker-compose

# OpenAI API Key (Required for AI features)
OPENAI_API_KEY=your_openai_api_key_here

# GLM API Key (Optional for design suggestions)
GLM_API_KEY=

# MongoDB Connection
MONGODB_URI=mongodb://mongodb:27017/reportgenerator

# Mongo Express (optional - for database management)
MONGO_EXPRESS_USER=admin
MONGO_EXPRESS_PASSWORD=password
